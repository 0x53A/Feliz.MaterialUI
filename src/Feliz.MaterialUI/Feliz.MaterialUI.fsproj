<Project Sdk="Microsoft.NET.Sdk">
  
  <PropertyGroup>
    <TargetFramework>netstandard2.0</TargetFramework>
    <GenerateDocumentationFile>true</GenerateDocumentationFile>
    <GeneratePackageOnBuild>true</GeneratePackageOnBuild>
    <Description>Feliz-style Fable bindings for Material-UI.</Description>
    <PackageProjectUrl>https://github.com/cmeeren/Feliz.MaterialUI</PackageProjectUrl>
    <Authors>Christer van der Meeren</Authors>
    <PackageLicenseExpression>MIT</PackageLicenseExpression>
    <PackageTags>f# fable bindings feliz react material-ui materialui</PackageTags>
    <Version>0.8.0</Version>
    <PackageReleaseNotes>- Regenerated based on MUI API docs
- Breaking: buttonRef props removed (no longer exist in MUI API docs)
- Breaking: muiThemeProvider is now themeProvider (see issue #27)
- Updated for @material-ui/core 4.6
- Updated for @material-ui/lab 4.0.0-alpha.31 (new autocomplete component)</PackageReleaseNotes>
  </PropertyGroup>
  
  <ItemGroup>
    <Compile Include="Misc.fs" />
    <Compile Include="Bindings.fs" />
    <Compile Include="Autocomplete.fs" />
    <Compile Include="Hooks.fs" />
    <Compile Include="Colors.fs" />
    <Compile Include="Styles.fs" />
    <Compile Include="ThemeOverrides.fs" />
    <Compile Include="ThemeProps.fs" />
    <Compile Include="Classes.fs" />
    <Compile Include="Props.fs" />
    <Compile Include="Mui.fs" />
  </ItemGroup>
  
  <ItemGroup Condition="'$(Configuration)' == 'Release'">
    <Content Include="*.fsproj; *.fs" PackagePath="fable\" />
  </ItemGroup>
  
  <ItemGroup>
    <PackageReference Include="Feliz" Version="[0.67, 0.68)" />
  </ItemGroup>
  
  <PropertyGroup>
    <NpmDependencies>
      <NpmPackage Name="@material-ui/core" Version="gte 4.6 lt 5" ResolutionStrategy="max" />
      <NpmPackage Name="@material-ui/lab" Version="4.0.0-alpha.31" />
    </NpmDependencies>
  </PropertyGroup>
  
</Project>
