<Project Sdk="Microsoft.NET.Sdk">
  
  <PropertyGroup>
    <TargetFramework>netstandard2.0</TargetFramework>
    <GenerateDocumentationFile>true</GenerateDocumentationFile>
    <GeneratePackageOnBuild>true</GeneratePackageOnBuild>
    <Description>Feliz-style Fable bindings for Material-UI.</Description>
    <PackageProjectUrl>https://github.com/cmeeren/Feliz.MaterialUI</PackageProjectUrl>
    <Authors>Christer van der Meeren</Authors>
    <PackageLicenseExpression>MIT</PackageLicenseExpression>
    <PackageTags>f# fable bindings feliz react material-ui materialui</PackageTags>
    <Version>0.3.1</Version>
    <PackageReleaseNotes>Updated Feliz dependency and pinned the version to avoid breaking 0.x changes</PackageReleaseNotes>
  </PropertyGroup>
  
  <ItemGroup>
    <Compile Include="ThemeOverrides.fs" />
    <Compile Include="ThemeProps.fs" />
    <Compile Include="Bindings.fs" />
    <Compile Include="Hooks.fs" />
    <Compile Include="Colors.fs" />
    <Compile Include="Classes.fs" />
    <Compile Include="Styles.fs" />
    <Compile Include="Props.fs" />
    <Compile Include="Mui.fs" />
  </ItemGroup>
  
  <ItemGroup Condition="'$(Configuration)' == 'Release'">
    <Content Include="*.fsproj; *.fs" PackagePath="fable\" />
  </ItemGroup>
  
  <ItemGroup>
    <PackageReference Include="Feliz" Version="[0.56, 0.57)" />
  </ItemGroup>
  
  <PropertyGroup>
    <NpmDependencies>
      <NpmPackage Name="@material-ui/core" Version="gte 4.5 lt 5" ResolutionStrategy="max" />
      <NpmPackage Name="@material-ui/lab" Version="4.0.0-alpha.28" />
    </NpmDependencies>
  </PropertyGroup>
  
</Project>
